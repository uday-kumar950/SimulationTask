


<p style="color: green"><%= notice %></p>

<h1>Employees</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>dob</th>
      <th>Email</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |e| %>
      <tr>
        <td><%= e.emp_name %></td>
        <td><%= e.dob %></td>
        <td><%= e.email %></td>
        <td><%= link_to "Show this employee", e %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to "New employee", new_employee_path %>